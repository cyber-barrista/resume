{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "rangeStrategy": "pin",
  "commitMessagePrefix": "[Common] ",
  "prConcurrentLimit": 3,
  "labels": [
    "common"
  ],
  "schedule": [
    "on the 15th day of the month"
  ],
  "lockFileMaintenance": {
    "enabled": true
  },
  "packageRules": [
    {
      "digest": {
        "enabled": false
      }
    },
    {
      "matchUpdateTypes": [
        "*"
      ],
      "groupName": "all",
      "matchPackageNames": [
        "*"
      ]
    }
  ]
}